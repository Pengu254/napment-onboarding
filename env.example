# =============================================================================
# NAPMENT ONBOARDING - Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your values
# =============================================================================

# -----------------------------------------------------------------------------
# Domain
# -----------------------------------------------------------------------------
DOMAIN=bobbi.live

# -----------------------------------------------------------------------------
# Supabase (BOBBI Build Platform Database)
# -----------------------------------------------------------------------------
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key

# -----------------------------------------------------------------------------
# Shopify OAuth (BOBBI App from Shopify Partners)
# -----------------------------------------------------------------------------
SHOPIFY_CLIENT_ID=your-shopify-app-client-id
SHOPIFY_CLIENT_SECRET=your-shopify-app-client-secret

# -----------------------------------------------------------------------------
# URLs (usually don't need to change)
# -----------------------------------------------------------------------------
OAUTH_CALLBACK_URL=https://onboarding-api.bobbi.live/auth/shopify/callback
FRONTEND_URL=https://onboarding.bobbi.live
